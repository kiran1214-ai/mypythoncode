# way 1
import random as ra
c=0
while c<=5:
    num = ra.randint(100,999)
    if num% 5==0:
        print(num)
        c+=1

##########################################
# way 2
print("way 2 is below")
for i in range(5):
    # randrange is a random method which takes 3 parameters : 1 parameter is starting range , 2nd parameter is ending range ,3rd parameter is step
    print(ra.randrange(100,999,5))
